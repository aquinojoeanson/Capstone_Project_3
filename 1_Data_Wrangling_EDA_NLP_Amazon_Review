        "word_bank = [\"nook\", \"phone\", \"tv\", \"good\", \"price\"]\n",
        "\n",
        "for word in word_bank[:]:\n",
        "    related_vec = word_vec.wv.most_similar(word, topn=5)\n",
        "    related_words = np.array(related_vec)[:,0]\n",
        "    word_bank.extend(related_words)\n",
