        "from gensim.models.tfidfmodel import TfidfModel\n",
        "\n",
        "tfidf = TfidfModel(bow)\n",
        "\n",
        "for idx, weight in tfidf[bow[0]]:\n",
        "    print(f\"Word: {vocabulary.get(idx)}, Weight: {weight:.3f}\")"
      ]
