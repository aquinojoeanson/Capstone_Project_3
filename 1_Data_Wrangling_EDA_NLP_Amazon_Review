        "%%time\n",
        "\n",
        "tokenized_array = np.array(tokenized)\n",
        "word_vec_df = word_vec_df.set_index(['otr', 'trucker', 'worked', 'scale', 'ect', 'exception'])\n",
        "\n",
        "model_array = np.array([word_vec_df.loc[doc].mean(axis=0) for doc in tokenized_array])\n",
        "\n",
        "\n"
